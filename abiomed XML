      <script type= "text/JavaScript">
        // Get the preferences for the current user
        var prefs = new gadgets.Prefs();
        function abiomed_youtube_channel() {
          var html="Abiomed YouTube Channel!";
          document.getElementById("content_div").style.backgroundColor=pres.getString
          document.getElementById("content_div").innerHTML = html;
          gadgets.window.adjustHeight();
        }
        gadgets.until.registerOnLoadHandler(abiomed_youtube_channel);
      </script>
      <div id= "content_div"></div>
